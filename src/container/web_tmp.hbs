{{!--
Це код container файлу для summary сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

<div>

  <div> {{web.languages.[0].name}}</div>
  <div> {{web.languages.[1].name}}</div>
  <div> {{web.languages.[2].name}}</div>


  <div> {{web.languages.[0].elements.[0].name}}</div>
  <div> {{web.languages.[0].elements.[0].attributes.[0].name}}</div>

  <div> {{web.languages.[1].properties.[0].name}}</div>
  <div> {{web.languages.[1].properties.[0].values.[0].value}}</div>

  <div> {{web.languages.[2].functions.[0].name}}</div>
  <div> {{web.languages.[2].functions.[0].parameters.[0].name}}</div>


  {{#with web}}

  <main>


    {{!-- <section>
      {{#each languages}}
      {{> title data = name big=true}}
      {{/each}}
    </section> --}}

    <section>
      {{#each languages}}
      <div> {{name}} </div>
      <div> {{name}} , {{version}} , {{description}} , </div>

      {{#each elements }}
      <div> {{name}} {{description}} </div>

      {{!-- {{#each properties}}
      <div> {{name}} {{description}} </div>
      {{/each}} --}}

      {{#each attributes}}
      <div> {{name}} {{description}} </div>
      {{/each}}
      {{!-- ------- CSS ----------- --}}

      {{#each properties}}
      <div> {{name}} {{description}} </div>
      {{#each values}}
      <div> {{value}} {{description}} </div>
      {{/each}}

      {{!-- ------- JavaScript ----------- --}}
      {{#each functions}}
      <div> {{name}} {{description}} </div>
      {{#each parameters}}
      <div> {{name}} {{type}} {{description}} </div>






      {{/each}}
      {{/each}}
      {{/each}}
      {{/each}}
      {{/each}}
    </section>



  </main>
  {{/with}}

</div>






























{{!--
Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs
Адже наш layout/default.hbs має вже структуру HTML
--}}